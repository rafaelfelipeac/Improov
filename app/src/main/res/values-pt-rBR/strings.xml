    <string name="tips_goal_type_list_message">Para objetivos que consistem em concluir diferentes itens de uma lista. Cada item concluído representa 1 ponto para o objetivo final.</string>
    <string name="tips_goal_type_counter_title">Contador</string>
    <string name="tips_goal_type_counter_message">Ideal para objetivos onde os valores a serem somados ou subtraídos sejam sempre os mesmos.</string>
    <string name="tips_goal_type_final_title">Valor final</string>
    <string name="tips_goal_type_final_message">Destinado a objetivos onde os valores a serem somados ou subtraídos mudem constantemente.</string>

